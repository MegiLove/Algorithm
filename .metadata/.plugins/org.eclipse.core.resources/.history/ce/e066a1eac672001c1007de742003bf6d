package com.sist.algo;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Stack;
import java.util.Scanner;

public class queue {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=0;
		int cnt=0;
		Scanner sc= new Scanner(System.in);
		System.out.println("빌딩 개수를 입력하세요");
		n= sc.nextInt();
		if(n>=1 && n<=80000) {
			int []h= new int[80000];
			for(int i=0; i<=n-1; i++){
				System.out.println("각 빌딩의 높이들을 입력하세요");
				h[i]=sc.nextInt();
								
			}
			for(int i = 0; i<n ;i++) {
				for(int j =i +1 ;j < n ;j++) {
					if(h[i]>h[j]) {
						cnt++;
					}
					else {
						break;
					}
				}
			}
			
			
		}
		
		System.out.println("빌딩의 총 개수는 "+cnt+"개 이다");
		}
	
	}


